  "main": "index.js",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-plugin-json": "^1.2.0",
    "mocha": "^2.0.0",
    "nconf": "^0.8.4"
